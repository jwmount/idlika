<div id="li_body">

<div id="li_content">

<h1 class="login"></h1>
<br/>
<% form_for @user_session do |f| %>
  <%= f.error_messages %>

  <div id="addgift_form">
  <div class="clearer"></div>
  <div class="aglabel" style="width:100px;"><%= f.label :username %></div>
  <div class="agfield"><%= f.text_field :username, :class=>"agtext" %></div>
  <div class="clearer"></div>

  <div id="addgift_form">
  <div class="clearer"></div>
  <div class="aglabel" style="width:100px;"><%= f.label :password %></div>
  <div class="agfield"><%= f.password_field :password, :class=>"agtext" %></div>
  <div class="clearer"></div>

  <p><%= f.submit "Submit" %></p>
</div><!-- addgift_form -->

<% end %>

<br /></br />
<%= link_to 'Back', gifts_path %>
<br /></br />
<br /></br />
<br /></br />
</div><!-- li_content -->
<div class="clearer"></div>
</div><!-- li_body -->

