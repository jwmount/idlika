<h1>New giver</h1>

<% form_for(@giver) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :gift_id %><br />
    <%= f.text_field :gift_id %>
  </p>
  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :status %><br />
    <%= f.text_area :status %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', givers_path %>